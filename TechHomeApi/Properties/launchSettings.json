        "ASPNETCORE_ENVIRONMENT": "Development",
        "DATABASE_CONNECTION_STRING": "Server=localhost\\SQLEXPRESS;Database=master;Trusted_Connection=True;"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5177"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DATABASE_CONNECTION_STRING":"Server=localhost\\SQLEXPRESS;Database=master;Trusted_Connection=True;"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7006;http://localhost:5177"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DATABASE_CONNECTION_STRING": "Server=localhost\\SQLEXPRESS;Database=master;Trusted_Connection=True;"
      }
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
